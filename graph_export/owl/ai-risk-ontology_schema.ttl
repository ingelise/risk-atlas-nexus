nexus:IncidentConcludedclass a owl:Class ;
    rdfs:label "IncidentConcludedclass" ;
    rdfs:subClassOf nexus:IncidentStatus ;
    skos:exactMatch <https://w3id.org/dpv#IncidentConcludedclass> ;
    skos:inScheme nexus:ai_risk .

nexus:IncidentHaltedclass a owl:Class ;
    rdfs:label "IncidentHaltedclass" ;
    rdfs:subClassOf nexus:IncidentStatus ;
    skos:exactMatch <https://w3id.org/dpv#IncidentHaltedclass> ;
    skos:inScheme nexus:ai_risk .

nexus:IncidentMitigatedclass a owl:Class ;
    rdfs:label "IncidentMitigatedclass" ;
    rdfs:subClassOf nexus:IncidentStatus ;
    skos:exactMatch <https://w3id.org/dpv#IncidentMitigatedclass> ;
    skos:inScheme nexus:ai_risk .

nexus:IncidentNearMissclass a owl:Class ;
    rdfs:label "IncidentNearMissclass" ;
    rdfs:subClassOf nexus:IncidentStatus ;
    skos:exactMatch <https://w3id.org/dpv#IncidentNearMissclass> ;
    skos:inScheme nexus:ai_risk .

nexus:IncidentOngoingclass a owl:Class ;
    rdfs:label "IncidentOngoingclass" ;
    rdfs:subClassOf nexus:IncidentStatus ;
    skos:exactMatch <https://w3id.org/dpv#IncidentOngoingclass> ;
    skos:inScheme nexus:ai_risk .

nexus:Input a owl:Class ;
    rdfs:label "Input" ;
    rdfs:subClassOf nexus:Entity ;
    skos:definition "Input for which the system or component generates output." ;
    skos:exactMatch airo:Input ;
    skos:inScheme nexus:ai_system .

nexus:Principle a owl:Class ;
    rdfs:label "Principle" ;
    skos:definition "A representation of values or norms that must be taken into consideration when conducting activities" ;
    skos:exactMatch <https://w3id.org/dpv#Principle> ;
    skos:inScheme nexus:common .

nexus:Questionnaire a owl:Class ;
    rdfs:label "Questionnaire" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:composed_of ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:composed_of ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:Question ;
            owl:onProperty nexus:composed_of ],
        nexus:AiEval ;
    skos:definition "A questionnaire groups questions" ;
    skos:inScheme nexus:ai_eval .

nexus:ai-risk-ontology.owl.ttl a owl:Ontology ;
    rdfs:label "ai-risk-ontology" ;
    dcterms:license "https://www.apache.org/licenses/LICENSE-2.0.html" ;
    pav:version "0.5.0" ;
    skos:definition "An ontology describing AI systems and their risks" .

nexus:hasTerm a owl:ObjectProperty ;
    rdfs:label "hasTerm" ;
    rdfs:range nexus:Term ;
    skos:definition "Indicates terms associated with a vocabulary" ;
    skos:inScheme nexus:common .

nexus:isDeployedBy a owl:ObjectProperty ;
    rdfs:label "isDeployedBy" ;
    rdfs:range nexus:Organization ;
    skos:definition "A relationship indicating that an entity has been deployed by an organization." ;
    skos:inScheme nexus:eu_ai_act .

nexus:isDistributedBy a owl:ObjectProperty ;
    rdfs:label "isDistributedBy" ;
    rdfs:range nexus:Organization ;
    skos:definition "A relationship indicating that an entity has been distributed by an organization." ;
    skos:inScheme nexus:eu_ai_act .

nexus:isImportedBy a owl:ObjectProperty ;
    rdfs:label "isImportedBy" ;
    rdfs:range nexus:Organization ;
    skos:definition "A relationship indicating that an entity has been imported by an organization." ;
    skos:inScheme nexus:eu_ai_act .

nexus:training_data_preprocessing a owl:ObjectProperty ;
    rdfs:label "training_data_preprocessing" ;
    rdfs:range nexus:DataPreprocessing ;
    skos:definition "relationship indicating data preprocessing steps performed on training data sets to ensure high quality training data." ;
    skos:inScheme nexus:ai_system .

nexus:validated_by a owl:ObjectProperty ;
    rdfs:label "validated_by" ;
    rdfs:range nexus:AiModelValidation ;
    skos:definition "A relationship indicating the model validation steps after AI model training." ;
    skos:inScheme nexus:ai_system .

nexus:AiModel a owl:Class ;
    rdfs:label "AiModel" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty nexus:architecture ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:architecture ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:RiskControl ;
            owl:onProperty nexus:hasRiskControl ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:carbon_emitted ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:power_consumption_w ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:carbon_emitted ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:architecture ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:AiEvalResult ;
            owl:onProperty nexus:hasEvaluation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:gpu_hours ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:power_consumption_w ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasRiskControl ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
            owl:onProperty nexus:power_consumption_w ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:integer [ a rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
            owl:onProperty nexus:gpu_hours ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:gpu_hours ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( xsd:float [ a rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
            owl:onProperty nexus:carbon_emitted ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasEvaluation ],
        nexus:BaseAi ;
    skos:definition "A base AI Model class. No assumption about the type (SVM, LLM, etc.). Subclassed by model types (see LargeLanguageModel)." ;
    skos:inScheme nexus:ai_system .

nexus:AiModelValidation a owl:Class ;
    rdfs:label "AiModelValidation" ;
    rdfs:subClassOf nexus:AiLifecyclePhase ;
    skos:definition "AI model validation steps that have been performed after the model training to ensure high AI model quality." ;
    skos:inScheme nexus:ai_system .

nexus:AiSystem a owl:Class ;
    rdfs:label "AiSystem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nexus:EuAiRiskCategory ;
            owl:onProperty nexus:hasEuRiskCategory ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:isComposedOf ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:hasEuRiskCategory ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:BaseAi ;
            owl:onProperty nexus:isComposedOf ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasEuRiskCategory ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:AiSystemType ;
            owl:onProperty nexus:hasEuAiSystemType ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasEuAiSystemType ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:hasEuAiSystemType ],
        nexus:BaseAi ;
    skos:definition "A compound AI System composed of one or more AI capablities. ChatGPT is an example of an AI system which deploys multiple GPT AI models." ;
    skos:exactMatch airo:AISystem ;
    skos:inScheme nexus:ai_system .

nexus:DataPreprocessing a owl:Class ;
    rdfs:label "DataPreprocessing" ;
    rdfs:subClassOf nexus:AiLifecyclePhase ;
    skos:definition "Data transformations, such as PI filtering, performed to ensure high quality of AI model training data." ;
    skos:inScheme nexus:ai_system .

nexus:Fact a owl:Class ;
    rdfs:label "Fact" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:evidence ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:evidence ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty nexus:value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty nexus:evidence ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty nexus:value ] ;
    skos:definition "A fact about something, for example the result of a measurement. In addition to the value, evidence is provided." ;
    skos:exactMatch <http://schema.org/Statement> ;
    skos:inScheme nexus:common .

nexus:Question a owl:Class ;
    rdfs:label "Question" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty nexus:text ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:text ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty nexus:text ],
        nexus:AiEval ;
    skos:definition "An evaluation where a question has to be answered" ;
    skos:inScheme nexus:ai_eval .

nexus:Stakeholder a owl:Class ;
    rdfs:label "Stakeholder" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nexus:StakeholderGroup ;
            owl:onProperty nexus:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:RiskTaxonomy ;
            owl:onProperty nexus:isDefinedByTaxonomy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:isDefinedByTaxonomy ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:isPartOf ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:isDefinedByTaxonomy ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:isPartOf ],
        nexus:Entity ;
    skos:definition "An AI system stakeholder for Responsible AI governance (e.g., AI governors, users, consumers)." ;
    skos:inScheme nexus:ai_csiro_rai .

<https://ibm.github.io/risk-atlas-nexus/ontology/AdapterType#ALORA> a owl:Class ;
    rdfs:label "ALORA" ;
    rdfs:subClassOf nexus:AdapterType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AdapterType#LORA> a owl:Class ;
    rdfs:label "LORA" ;
    rdfs:subClassOf nexus:AdapterType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AdapterType#X-LORA> a owl:Class ;
    rdfs:label "X-LORA" ;
    rdfs:subClassOf nexus:AdapterType .

nexus:AiLifecyclePhase a owl:Class ;
    rdfs:label "AiLifecyclePhase" ;
    rdfs:subClassOf nexus:Entity ;
    skos:definition "A Phase of AI lifecycle which indicates evolution of the system from conception through retirement." ;
    skos:exactMatch airo:AILifecyclePhase ;
    skos:inScheme nexus:ai_system .

nexus:AiProvider a owl:Class ;
    rdfs:label "AiProvider" ;
    rdfs:subClassOf nexus:Organization ;
    skos:definition "A provider under the AI Act is defined by Article 3(3) as a natural or legal person or body that develops an AI system or general-purpose AI model or has an AI system or general-purpose AI model developed; and places that system or model on the market, or puts that system into service, under the provider's own name or trademark, whether for payment or free for charge." ;
    skos:exactMatch airo:AIProvider ;
    skos:inScheme nexus:ai_system .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#GPAI> a owl:Class ;
    rdfs:label "GPAI" ;
    rdfs:subClassOf nexus:AiSystemType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#GPAI_OS> a owl:Class ;
    rdfs:label "GPAI_OS" ;
    rdfs:subClassOf nexus:AiSystemType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#HIGH_RISK> a owl:Class ;
    rdfs:label "HIGH_RISK" ;
    rdfs:subClassOf nexus:AiSystemType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#MILITARY_SECURITY> a owl:Class ;
    rdfs:label "MILITARY_SECURITY" ;
    rdfs:subClassOf nexus:AiSystemType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#PROHIBITED> a owl:Class ;
    rdfs:label "PROHIBITED" ;
    rdfs:subClassOf nexus:AiSystemType .

<https://ibm.github.io/risk-atlas-nexus/ontology/AiSystemType#SCIENTIFIC_RD> a owl:Class ;
    rdfs:label "SCIENTIFIC_RD" ;
    rdfs:subClassOf nexus:AiSystemType .

nexus:Consequence a owl:Class ;
    rdfs:label "Consequence" ;
    rdfs:subClassOf nexus:Entity ;
    skos:exactMatch <https://w3id.org/dpv#Consequence> ;
    skos:inScheme nexus:ai_risk .

<https://ibm.github.io/risk-atlas-nexus/ontology/EuAiRiskCategory#EXCLUDED> a owl:Class ;
    rdfs:label "EXCLUDED" ;
    rdfs:subClassOf nexus:EuAiRiskCategory .

<https://ibm.github.io/risk-atlas-nexus/ontology/EuAiRiskCategory#HIGH_RISK> a owl:Class ;
    rdfs:label "HIGH_RISK" ;
    rdfs:subClassOf nexus:EuAiRiskCategory .

<https://ibm.github.io/risk-atlas-nexus/ontology/EuAiRiskCategory#HIGH_RISK_EXCEPTION> a owl:Class ;
    rdfs:label "HIGH_RISK_EXCEPTION" ;
    rdfs:subClassOf nexus:EuAiRiskCategory .

<https://ibm.github.io/risk-atlas-nexus/ontology/EuAiRiskCategory#LIMITED_OR_LOW_RISK> a owl:Class ;
    rdfs:label "LIMITED_OR_LOW_RISK" ;
    rdfs:subClassOf nexus:EuAiRiskCategory .

<https://ibm.github.io/risk-atlas-nexus/ontology/EuAiRiskCategory#PROHIBITED> a owl:Class ;
    rdfs:label "PROHIBITED" ;
    rdfs:subClassOf nexus:EuAiRiskCategory .

nexus:LLMIntrinsic a owl:Class ;
    rdfs:label "LLMIntrinsic" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasRelatedTerm ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasDocumentation ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:Adapter ;
            owl:onProperty nexus:hasAdapter ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasAdapter ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:Documentation ;
            owl:onProperty nexus:hasDocumentation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:isDefinedByVocabulary ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:intersectionOf ( [ owl:unionOf ( nexus:RiskConcept nexus:Term ) ] nexus:Term ) ] ;
            owl:onProperty nexus:hasRelatedTerm ],
        [ a owl:Restriction ;
            owl:allValuesFrom nexus:Vocabulary ;
            owl:onProperty nexus:isDefinedByVocabulary ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty nexus:isDefinedByVocabulary ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty nexus:hasRelatedRisk ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:intersectionOf ( [ owl:unionOf ( nexus:RiskConcept nexus:Term ) ] nexus:Risk ) ] ;
            owl:onProperty nexus:hasRelatedRisk ],
        nexus:Entity ;
    skos:definition "A capability that can be invoked through a well-defined API that is reasonably stable and independent of how the LLM intrinsic itself is implemented." ;
    skos:exactMatch <https://w3id.org/dpv/ai#Capability> ;
    skos:inScheme nexus:ai_intrinsic .

nexus:LargeLanguageModelFamily a owl:Class ;
    rdfs:label "LargeLanguageModelFamily" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nexus:Documentation ;
            owl:onProperty nexus:hasDocumentation ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
